{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "seedsnote",
  "title": "Seedsnote",
  "description": "A Seedsnote extension, capture your idea instantly.",
  "icon": "logo.png",
  "author": "Ironben",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "seed",
      "title": "Seed",
      "description": "A Seedsnote extension, capture your idea instantly.",
      "subtitle": "Take a note",
      "mode": "view",
      "preferences": [
        {
          "name": "endpoint",
          "title": "API",
          "description": "https://seedsnote.com/help/advanced/apitoken/",
          "type": "textfield",
          "required": true,
          "placeholder": "您的Seedsnote API"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.3",
    "@types/got": "^9.6.12",
    "got": "^13.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
